# Production Environment Variables for Vercel Deployment
# IMPORTANT: Rename this file to .env.production and fill in the values before deploying
# These values will be set in the Vercel dashboard under Project Settings > Environment Variables

# Supabase Configuration
# Use the same values as your current project or create a new production Supabase project
NEXT_PUBLIC_SUPABASE_URL=your-supabase-production-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-production-anon-key

# API Keys - IMPORTANT: Keep these secure by setting them in Vercel's Environment Variables
# Don't commit actual API keys to your repository
CLAUDE_API_KEY=YOUR_CLAUDE_API_KEY

# External API Keys
OPENWEATHERMAP_API_KEY=YOUR_OPENWEATHERMAP_API_KEY
NEWS_API_KEY=YOUR_NEWS_API_KEY
TICKETMASTER_API_KEY=YOUR_TICKETMASTER_API_KEY
ALPHAVANTAGE_API_KEY=YOUR_ALPHAVANTAGE_API_KEY

# OpenAI API Key
NEXT_PUBLIC_OPENAI_API_KEY=YOUR_OPENAI_API_KEY

# Base URL for the application
# This will be updated to your custom domain
NEXT_PUBLIC_BASE_URL=https://talkingobjects.ai

# Default Public User ID (used for public agents)
PUBLIC_USER_ID=00000000-0000-0000-0000-000000000000

# Production-specific settings
NODE_ENV=production 